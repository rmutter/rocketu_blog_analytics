[group:rocketu_blog_analytics]
programs=gunicorn_rocketu_blog_analytics

[program:gunicorn_rocketu_blog_analytics]
command={{ virtualenv_path }}/bin/gunicorn -c gunicorn.conf.py -p gunicorn.pid wsgi:application --pythonpath {{ project_path }}/rocketu_blog_analytics
directory={{ project_path }}
user=ubuntu
autostart=true
autorestart=true
redirect_stderr=true